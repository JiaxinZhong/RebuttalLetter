@article{Zhang2022NonAbelianBraidingPhotonic,
  title = {Non-{{Abelian}} Braiding on Photonic Chips},
  author = {Zhang, Xu-Lin and Yu, Feng and Chen, Ze-Guo and Tian, Zhen-Nan and Chen, Qi-Dai and Sun, Hong-Bo and Ma, Guancong},
