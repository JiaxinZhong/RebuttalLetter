@article{Liu2023AcousticTopologicalMetamaterials,
  title = {Acoustic {{Topological Metamaterials}} of {{Large Winding Number}}},
  author = {Liu, Han and Huang, Xueqin and Yan, Mou and Lu, Jiuyang and Deng, Weiyin and Liu, Zhengyou},
  date = {2023-05-09},
  journaltitle = {Phys. Rev. Appl.},
