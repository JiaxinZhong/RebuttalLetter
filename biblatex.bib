@article{Firtha2018GeneralRelationWave,
  title = {On the {{General Relation}} of {{Wave Field Synthesis}} and {{Spectral Division Method}} for {{Linear Arrays}}},
  author = {Firtha, Gergely and Fiala, Péter and Schultz, Frank and Spors, Sascha},
