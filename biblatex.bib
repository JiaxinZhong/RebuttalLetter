@inproceedings{Shi2013UsingLengthlimitedParametric,
  title = {Using Length-Limited Parametric Source in Active Noise Control Applications},
  author = {Shi, Chuang and Gan, Woon-Seng},
  date = {2013-07-10},
  location = {{Bangkok, Thailand}},
  abstract = {In an active noise control (ANC) application, a conventional loudspeaker is deployed as the control source to transmit an anti-noise wave, which eventually suppress the noise in the targeted area and result in a quite zone. A well-known drawback of such an ANC application is that the sound pressure level at locations other than the control point may undesirably in-crease due to the loudspeaker's omnidirectional beampattern. Hence, the feasibility of using a parametric loudspeaker as the control source in an ANC application has recently been studied and validated in experiments. However, the low-frequency sound field created by a parametric loudspeaker can reach up to 8 meters (given by the absorption distance of 40 kHz wave in air) from the surface of its ultrasonic emitter. The spillover of the anti-noise wave remains unsolvable within this region. To address this problem, this paper proposes to use the length-limited parametric source, which is recently reported by Kamakura et al. [Applied Acoustics 73, 1231 (2012)], in ANC applications. The length-limited parametric source consists of two parametric loudspeakers on the same axis. Different carrier frequencies are applied to the two parametric loudspeakers to result in different absorption lengths. By adjusting the input amplitudes and phases of the two parametric loudspeakers, their secondary sound waves can suppress each other in the overlapped sound fields but retain in the difference area. Thus, using the length-limited parametric source, the maximum noise cancellation can be achieved only at a particular spot, without disturbing neighboring areas. A new doubly-nested parameter adjustment structure is introduced for cancelling the secondary sound field outside the control spot and yet achieve noise reduction within the spot. The Khokhlov-Zabolotskaya-Kuznetsov (KZK) equation is adopted to model the parametric loudspeaker in the simulation, and the results show the potentials of the proposed method.},
  eventtitle = {20th {{International Congress}} on {{Sound}} and {{Vibration}} ({{ICSV}})}
}

