  shorttitle = {3-{{D}} Imaging Using Row-Column-Addressed Arrays with Integrated Apodization - Part i},
  author = {Rasmussen, Morten Fischer and Christiansen, Thomas Lehrmann and Thomsen, Erik Vilain and Jensen, Jørgen Arendt},
