@article{Mitri2008TheoryAcousticRadiation,
  title = {Theory of the Acoustic Radiation Force Exerted on a Sphere by Standing and Quasistanding Zero-Order {{Bessel}} Beam Tweezers of Variable Half-Cone Angles},
