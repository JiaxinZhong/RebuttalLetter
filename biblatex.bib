@article{Luo1995MonteCarloInvestigation,
  title = {A {{Monte Carlo}} Investigation of Dual-Energy-Window Scatter Correction for Volume-of-Interest Quantification in {{99Tcm SPECT}}},
  author = {Luo, J. Q. and Koral, K. F. and Ljungberg, M. and Floyd, Jr., C. E. and Jaszczak, R. J.},
  date = {1995-01},
  journaltitle = {Phys Med Biol},
