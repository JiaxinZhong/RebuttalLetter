@article{Xia2018ProgrammableCodingAcoustic,
  title = {Programmable {{Coding Acoustic Topological Insulator}}},
  author = {Xia, Jian-Ping and Jia, Ding and Sun, Hong-Xiang and Yuan, Shou-Qi and Ge, Yong and Si, Qiao-Rui and Liu, Xiao-Jun},
  date = {2018},
  journaltitle = {Advanced Materials},
