@article{Zhou2017OpticalIsolationNonlinear,
  title = {Optical Isolation with Nonlinear Topological Photonics},
  author = {Zhou, Xin and Wang, You and Leykam, Daniel and Chong, Y. D.},
  date = {2017-09},
  journaltitle = {New J. Phys.},
  volume = {19},
