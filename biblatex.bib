@article{Zhuang2023SteerableNonparaxialGaussian,
  title = {A Steerable Non-Paraxial {{Gaussian}} Beam Expansion for a Steerable Parametric Array Loudspeaker},
  author = {Zhuang, Tao and Zhong, Jiaxin and Niu, Feng and Karimi, Mahmoud and Kirby, Ray and Lu, Jing},
