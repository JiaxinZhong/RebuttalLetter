@article{He2020ObservationQuadraticWeyl,
  title = {Observation of Quadratic {{Weyl}} Points and Double-Helicoid Arcs},
  author = {He, Hailong and Qiu, Chunyin and Cai, Xiangxi and Xiao, Meng and Ke, Manzhu and Zhang, Fan and Liu, Zhengyou},
  date = {2020-04-14},
  journaltitle = {Nat Commun},
  volume = {11},
