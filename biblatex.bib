@inproceedings{Jimenez2022RotatingAcousticDrills,
  title = {Rotating Acoustic Drills by the Interference of Detuned Vortices},
  booktitle = {2022 {{IEEE International Ultrasonics Symposium}} ({{IUS}})},
  author = {Jiménez, Noé and González-Mateo, Enrique and Camarena, Francisco and Staliunas, Kestitis},
  date = {2022-10},
