@article{Martin2012ElasticShellsHighcontrast,
  title = {Elastic Shells with High-Contrast Material Properties as Acoustic Metamaterial Components},
  author = {Martin, Theodore P. and Layman, Christopher N. and Moore, Kimberly M. and Orris, Gregory J.},
  date = {2012-04-19},
  journaltitle = {Phys. Rev. B},
