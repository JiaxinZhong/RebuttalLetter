@article{Konwar2020LeveragingOrthogonalityZernike,
  title = {Leveraging the Orthogonality of {{Zernike}} Modes for Robust Free-Space Optical Communication},
  author = {Konwar, Santanu and Boruah, Bosanta R.},
  date = {2020-11-06},
