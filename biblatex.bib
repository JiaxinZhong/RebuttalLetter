@article{Tennyson2014DatabaseWaterTransitions,
  title = {A Database of Water Transitions from Experiment and Theory ({{IUPAC Technical Report}})},
  author = {Tennyson, Jonathan and Bernath, Peter F. and Brown, Linda R. and Campargue, Alain and Császár, Attila G. and Daumont, Ludovic and Gamache, Robert R. and Hodges, Joseph T. and Naumenko, Olga V. and Polyansky, Oleg L. and Rothman, Laurence S. and Vandaele, Ann Carine and Zobov, Nikolai F.},
  date = {2014-01-22},
  journaltitle = {Pure and Applied Chemistry},
