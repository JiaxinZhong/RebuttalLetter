@article{Donnelly1984ShapeRibuloseBisphosphate,
  title = {The Shape of Ribulose Bisphosphate Carboxylase/Oxygenase in Solution as Inferred from Small Angle Neutron Scattering},
  author = {Donnelly, M. I. and Hartman, F. C. and Ramakrishnan, V.},
