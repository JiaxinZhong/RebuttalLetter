@article{Arnela2021CharacterizationOmnidirectionalParametric,
  title = {Characterization of an Omnidirectional Parametric Loudspeaker with Exponential Sine Sweeps},
  author = {Arnela, Marc and Martínez-Suquía, Carme and Guasch, Oriol},
  date = {2021-11},
  journaltitle = {Appl. Acoust.},
  volume = {182},
