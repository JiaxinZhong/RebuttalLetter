@article{Shen2019ActiveAcousticMetasurface,
  title = {Active {{Acoustic Metasurface}}: {{Complete Elimination}} of {{Grating Lobes}} for {{High-Quality Ultrasound Focusing}} and {{Controllable Steering}}},
