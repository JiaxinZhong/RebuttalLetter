@article{Williams2010VectorIntensityReconstructions,
  title = {Vector Intensity Reconstructions in a Volume Surrounding a Rigid Spherical Microphone Arraya)},
  author = {Williams, Earl G. and Takashima, Kazuhiro},
  date = {2010-02-05},
  journaltitle = {The Journal of the Acoustical Society of America},
  volume = {127},
