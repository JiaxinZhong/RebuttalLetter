@article{LiuRemoteWatertoAirEavesdropping,
  title = {Remote {{Water-to-Air Eavesdropping With}} a {{Phase-Engineered Impedance Matching Metasurface}}},
  author = {Liu, Jingjing and Li, Zhengwei and Liang, Bin and Cheng, Jian-Chun and Alù, Andrea},
  journaltitle = {Advanced Materials},
  volume = {n/a},
  number = {n/a},
  pages = {2301799},
  issn = {1521-4095},
  doi = {10.1002/adma.202301799},
  url = {https://onlinelibrary.wiley.com/doi/abs/10.1002/adma.202301799},
  urldate = {2023-04-17},
  abstract = {Efficiently receiving underwater sound remotely from air is a long-standing challenge in acoustics hindered by the large impedance mismatch at the water-air interface. Here, a phase-engineered water-air impedance matching metasurface is proposed and experimentally demonstrated for remote and efficient water-to-air eavesdropping. The judiciously-designed metasurface with near-unity transmission efficiency, long monitoring distance and high mechanical stiffness is capable of making the water-air interface acoustically transparent and at the same time freewheelingly patterning the transmitted wavefront. This enables efficient control over the effective spatial location of a distant airborne sensor such that it can measure underwater signals with large signal-to-noise ratio as if placed close to the physical underwater source. Such airborne eavesdropping of underwater sound is experimentally demonstrated with a measured sensitivity enhancement of nearly 104 at 8 kHz, far from achievable with the current state-of-the-art methods. Moreover, the opportunities of using the proposed metasurface for cross-media orbital-angular-momentum-multiplexed communication and underwater acoustic window are also demonstrated. Our metasurface opens new avenues for communication and sensing in inhomogeneities with totally-reflective interfaces, which may be translated to nano-optics and radio-frequencies. This article is protected by copyright. All rights reserved},
  langid = {english},
  keywords = {metasurface,phase engineering,sound transparency,water-to-air eavesdropping},
  file = {C\:\\Users\\Jiaxin\\Zotero\\storage\\8YTA8CVG\\Liu et al_Remote Water-to-Air Eavesdropping With a Phase-Engineered Impedance Matching.pdf;C\:\\Users\\Jiaxin\\Zotero\\storage\\86QJ38NB\\adma.html}
}

