@article{Cheer2021ActiveFeedforwardControl,
  title = {Active Feedforward Control of Flexural Waves in an {{Acoustic Black Hole}} Terminated Beam},
  author = {Cheer, Jordan and Hook, K. and Daley, S.},
  date = {2021-01},
  journaltitle = {Smart Mater. Struct.},
  volume = {30},
