@article{Zhang2022ReviewNonHermitianSkin,
  title = {A Review on Non-{{Hermitian}} Skin Effect},
  author = {Zhang, Xiujuan and Zhang, Tian and Lu, Ming-Hui and Chen, Yan-Feng},
  date = {2022-12-31},
  journaltitle = {Advances in Physics: X},
  volume = {7},
  number = {1},
  pages = {2109431},
