@article{Moussa2023ObservationTemporalReflection,
  title = {Observation of Temporal Reflection and Broadband Frequency Translation at Photonic Time Interfaces},
  author = {Moussa, Hady and Xu, Gengyu and Yin, Shixiong and Galiffi, Emanuele and Ra’di, Younes and Alù, Andrea},
  date = {2023-03-13},
  journaltitle = {Nat. Phys.},
  pages = {1--6},
