@article{Memoli2017MetamaterialBricksQuantization,
  title = {Metamaterial Bricks and Quantization of Meta-Surfaces},
  author = {Memoli, Gianluca and Caleap, Mihai and Asakawa, Michihiro and Sahoo, Deepak R. and Drinkwater, Bruce W. and Subramanian, Sriram},
  date = {2017-02-27},
