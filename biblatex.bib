@article{Rozanova-Pierrat2009DerivationKhokhlovZabolotskayaKuznetsovKZK,
  title = {On the Derivation of the {{Khokhlov-Zabolotskaya-Kuznetsov}} ({{KZK}}) Equation and Validation of the {{KZK-approximation}} for Viscous and Non-Viscous Thermo-Elastic Media},
