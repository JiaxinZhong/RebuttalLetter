@article{Harne2016OrigamiAcousticsUsing,
  title = {Origami Acoustics: Using Principles of Folding Structural Acoustics for Simple and Large Focusing of Sound Energy},
  shorttitle = {Origami Acoustics},
  author = {Harne, Ryan L. and Lynd, Danielle T.},
  date = {2016-07},
  journaltitle = {Smart Mater. Struct.},
  volume = {25},
