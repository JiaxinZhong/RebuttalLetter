@article{Ji2019NoiseReductionCavity,
  title = {Noise Reduction inside a Cavity Coupled to a Flexible Plate with Embedded 2-{{D}} Acoustic Black Holes},
  author = {Ji, Hongli and Wang, Xiaodong and Qiu, Jinhao and Cheng, Li and Wu, Yipeng and Zhang, Chao},
