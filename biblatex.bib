@article{Sepehrirahnama2022WillisCouplingInducedAcoustic,
  title = {Willis {{Coupling-Induced Acoustic Radiation Force}} and {{Torque Reversal}}},
  author = {Sepehrirahnama, Shahrokh and Oberst, Sebastian and Chiang, Yan Kei and Powell, David A.},
