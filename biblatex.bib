@thesis{Chen2014ColumnRowParallelASICArchitecture,
  type = {Thesis},
  title = {A {{Column-Row-Parallel ASIC}} Architecture for {{3D}} Wearable / Portable Medical Ultrasonic Imaging},
  author = {Chen, Kailiang},
