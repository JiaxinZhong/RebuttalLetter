@article{Hlawitschka2011FastUltrasoundBeam,
  title = {Fast Ultrasound Beam Prediction for Linear and Regular Two-Dimensional Arrays},
  author = {Hlawitschka, Mario and Mcgough, Robert J. and Ferrara, Katherine W. and Kruse, Dustin E.},
